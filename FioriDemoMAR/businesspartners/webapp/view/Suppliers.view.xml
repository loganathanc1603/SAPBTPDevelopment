<mvc:View controllerName="ns.businesspartners.controller.Suppliers"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List id="idListSupplier" headerText="{LocalModel>/ListTitle}" items="{/BusinessPartnerSet}" growing="true" growingThreshold="50" updateFinished="onUpdateFinished">
                <items>
                    <StandardListItem id="idStandardListItems" title="{CompanyName}" description="{WebAddress}" ></StandardListItem>
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
