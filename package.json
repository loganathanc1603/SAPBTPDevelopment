{
  "name": "side-by-side-extensibility",
  "version": "1.0.0",
  "description": "CAPM Side By Side Extension | OnPremise",
  "repository": "https://github.com/loganathanc1603/SAPBTPDevelopment.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.8.1",
    "@sap-cloud-sdk/resilience": "^3.8.1",
    "@sap/cds": ">=5",
    "@sap/xssec": "^3",
    "express": "^4",
    "passport": "^0"
  },
  "devDependencies": {
    "sqlite3": "^5.0.4"
  },
  "scripts": {
    "start": "cds run",
    "cds_watch_hybrid": "cds watch --profile hybrid"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "SEPMRA_PROD_MAN": {
        "kind": "odata-v2",
        "model": "srv\\external\\SEPMRA_PROD_MAN",
        "credentials": {
          "destination": "ES5",
          "path": "/sap/opu/odata/sap/SEPMRA_PROD_MAN"
        }
      },
      "auth": {
        "kind": "dummy"
      },
      "northwind": {
        "kind": "odata-v2",
        "model": "srv\\external\\northwind",
        "credentials": {
          "destination": "Northwind",
          "path": "/v2/northwind/northwind.svc/"
        }
      },
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv\\external\\API_BUSINESS_PARTNER",
        "credentials": {
          "headers": {
            "APIKey": "oATX9A8l8FLwx9gnVAReO5W8lJOByMR0"
          },
          "url": "https://sandbox.api.sap.com/s4hanacloud/sap/opu/odata/sap/API_BUSINESS_PARTNER"
        }
      }
    }
  }
}
